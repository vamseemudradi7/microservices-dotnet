{
  "Logging":
  {
    "LogLevel":
    {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings":
  {
    "DefaultConnection": "Server=$MYSQL_HOST; Port=$MYSQL_PORT; Database=$MYSQL_DATABASE; User ID=$MYSQL_USER; Password=$MYSQL_PASSWORD"
  },
  "ServiceBus":
  {
    "ServiceBus_Products_Updates_Topic": "products.updates",
    "ServiceBus_Products_Deletions_Topic": "products.deletions",
    "eommcerce-servicebus-namespace": "your_servicebus_connection_string",

    "ServiceBus_Orders_Placed_Topic": "orders.placed",
    "ServiceBus_Orders_Placed_Products_Subscription": "orders.placed.products"
  }
}
